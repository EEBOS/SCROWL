{{#with . as | data |}}
<html lang="en" dir="ltr">
<head>
  <meta name='viewport' content='width=device-width, initial-scale=1'>
  <title>Scrowl Canvas</title>
  <script type="text/javascript" nonce="scrowl-manifest">
    window.__MANIFEST = {{{data.manifest}}};
  </script>
  <script type="module" src="{{{data.canvasUrl}}}" nonce="scrowl-canvas"></script>
</head>
<body>
  <div id="app">Hello World</div>
</body>
</html>
{{/with}}