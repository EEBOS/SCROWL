{{#with . as | data |}}
<html lang="en" dir="ltr">
<head>
  <meta name='viewport' content='width=device-width, initial-scale=1'>
  <title>Scrowl Canvas</title>
  <link rel="stylesheet" href="{{data.templateCss}}">
  <script type="text/javascript" src="./react.production.min.js"></script>
  <script type="text/javascript" src="./react-dom.production.min.js"></script>
  <script type="text/javascript" src="./react-bootstrap.min.js"></script>
  <script type="importmap">
    {
      "imports": {{{data.importList}}}
    }
  </script>
  <script type="text/javascript">
    window.__MANIFEST = {{{data.manifest}}};
  </script>
  <script type="module" src="./canvas.js"></script>
</head>
<body>
  <div id="app">Hello World</div>
</body>
</html>
{{/with}}