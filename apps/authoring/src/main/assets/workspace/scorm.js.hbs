{{#with . as | data |}}
import { createElement } from 'react';
import { createRoot } from 'react-dom';
import { App } from 'scrowl-player';
{{#each data.templates as | temp |}}
import { {{temp.component}} } from '{{temp.js}}';
{{/each}}

const container = document.getElementById('app');
const root = createRoot(container);
const templateList = {
  {{#each data.templates as | temp |}}
  '{{temp.component}}': {{temp.component}},
  {{/each}}
};

root.render(createElement(App, { templateList }, null));
{{/with}}