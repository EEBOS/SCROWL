{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/develop",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "deploy-gh": {
      "outputs": ["build/**"]
    },
    "lint": {
      "outputs": []
    },
    "start": {
      "cache": false
    }
  }
}
